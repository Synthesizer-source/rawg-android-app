<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="1dp"
                android:height="1dp" />
            <stroke
                android:width="2dp"
                android:color="@color/secondary_darker_red"
                android:dashWidth="4dp"
                android:dashGap="4dp" />
            <corners android:radius="4dp" />
            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_baseline_minus_24"
        android:gravity="center" />
</layer-list>